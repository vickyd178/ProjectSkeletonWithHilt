<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProjectSkeletonWithHilt" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#1E93F1</item>
        <item name="colorPrimaryVariant">#121212</item>
        <item name="colorOnPrimary">#121212</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#1E93F1</item>
        <item name="colorSecondaryVariant">#121212</item>
        <item name="colorOnSecondary">#000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="android:colorBackground">#212121</item>
        <item name="colorSurface">#121212</item>
        <item name="colorError">#CF6679</item>
        <item name="colorOnBackground">#FFFFFF</item>
        <item name="colorOnSurface">#FFFFFF</item>
        <item name="colorOnError">#000000</item>
    </style>


    <color name="colorEditTextBackgroundEnabled">#212121</color>
    <color name="colorEditTextBackgroundFocused">#372196F3</color>
    <color name="colorEditTextBackgroundDisabled">#888787</color>
    <color name="colorIconBackground">#212121</color>

    <style name="ThemeOverlay.ProjectSkeletonWithHilt.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_900</item>
        <item name="fullscreenTextColor">@color/light_blue_A400</item>
    </style>


</resources>