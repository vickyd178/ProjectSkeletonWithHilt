<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProjectSkeletonWithHilt" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#2196F3</item>
        <item name="colorPrimaryVariant">#1976D2</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#2196F3</item>
        <item name="colorSecondaryVariant">#1976D2</item>
        <item name="colorOnSecondary">#000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="android:colorBackground">#FFFFFF</item>
        <item name="colorSurface">#FFFFFF</item>
        <item name="colorError">#B00020</item>
        <item name="colorOnBackground">#000000</item>
        <item name="colorOnSurface">#000000</item>
        <item name="colorOnError">#FFFFFF</item>
    </style>

    <style name="Theme.ProjectSkeletonWithHilt.SplashScreen">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="Theme.ProjectSkeletonWithHilt.NoToolbar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <color name="colorEditTextBackgroundEnabled">#E2FDF7F7</color>
    <color name="colorEditTextBackgroundFocused">#372196F3</color>
    <color name="colorEditTextBackgroundDisabled">#BABABA</color>
    <color name="colorProfileEditBackground">#DDDDDD</color>
    <color name="colorIconBackground">#FFFFFF</color>
</resources>